{
	"printWidth": 80,
	"singleQuote": false,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"semi": true,
	"useTabs": true,
  "tabWidth": 1,
  "noMultipleEmptyLines": true,
	"proseWrap": "always",
	"overrides": [
		{
			"files": "*.md",
			"options": {
				"proseWrap": "never"
			}
		}
	]
}